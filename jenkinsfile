pipeline{
    agent any
    stages{
        stage("SCM-CHECKOUT"){
            steps{
                echo "helloworld"
            }
        }
        stage("BUILD"){
            steps{
                echo "HELLO MANASA"
            }
        }
        stage("DEPLOY"){
            steps{
                echo "SUCCESSFULLY"
            }
        }
        stage("SANITY-TEST"){
            steps{
                echo "ok"
            }
        }
    }
}